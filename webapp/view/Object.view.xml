<mvc:View
	controllerName="zjblessons.ControlTaskMoshkareva.controller.Object"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:semantic="sap.f.semantic">

	<Page title="Object Details">
        <content>
            <IconTabBar 
						select=".onIconTabBarSelect"
						expandable="false"
				id='idIconTabBar'
				headerMode="Inline"
				selectedKey="{= ${objectView>/sSelectedTab}}">
                <items>
                    <IconTabFilter
                        icon="sap-icon://form"
                        text="Form"
												key="Form">
                        <VBox>
												<List>
												<CustomListItem>
									<VBox class="supUiContentPadding">
										<Label
											text="MaterialID"
											labelFor="MaterialID"
										/>
										<Input
											id='MaterialID'
											editable="{= ${objectView>/bEditMode}}"
											value="{MaterialID}"
										/>
									</VBox>
								</CustomListItem>
								<CustomListItem>
									<VBox class="supUiContentPadding">
										<Label
											text="MaterialText"
											labelFor="MaterialText"
										/>
										<Input
											id=''
											editable="{= ${objectView>/bEditMode}}"
											value="{MaterialText}"
										/>
									</VBox>
								</CustomListItem>
												</List>
												</VBox>
                    </IconTabFilter>
                    <IconTabFilter
                        icon="sap-icon://notes"
                        text="Panel"
												key="Panel">
                        <Text text="Panel"/>
                    </IconTabFilter>
                </items>
            </IconTabBar>
        </content>
    </Page>

</mvc:View>